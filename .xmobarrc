Config { font = "xft:InconsolataForPowerline-10:normal"
, bgColor = "#111111"
, fgColor = "#aaaaaa"
, position = TopW L 100
, lowerOnStart = True
, commands = [ Run Memory ["-t","Mem: %"] 10
  , Run Battery ["-t","AC: <acstatus> <left>%  <watts>W <timeleft>"] 10
  , Run Mail [("inbox", "~/.mail/gmail/INBOX")
    , ("oxy.fi", "~/.mail/gmail/oxy.fi")
    , ("genero", "~/.mail/gmail/Genero")
    , ("arcada", "~/.mail/gmail/Arcada")
  ] "mail"
  , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
  , Run StdinReader
]
, sepChar = "%"
, alignSep = "}{"
, template = "%StdinReader%} {%mail%  %battery%  %date%  "
}
