defaults
auth on
port 465
tls on
logfile ~/.msmtp.log

account oxy
host mail.oxy.fi
from mail@oxy.fi
user mail+oxy.fi
tls_certcheck off
tls_starttls off
tls_min_dh_prime_bits 512

account arcada
host smtp.arcada.fi
from oskar.scholdstrom@arcada.fi
user scholdso
tls_certcheck off
tls_starttls off
tls_min_dh_prime_bits 512

account gmail
host smtp.gmail.com
from oskar.scholdstrom@gmail.com
user oskar.scholdstrom@gmail.com
tls_trust_file ~/.mutt/certificates/Equifax_Secure_CA.cert

account default : oxy
